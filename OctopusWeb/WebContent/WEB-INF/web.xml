<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>OctopusWeb</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>setup</servlet-name>
    <servlet-class>com.servlet.SetUp</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>console</servlet-name>
    <servlet-class>com.servlet.OConsole</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>console</servlet-name>
    <url-pattern>/console</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>map</servlet-name>
    <servlet-class>com.servlet.OMap</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>map</servlet-name>
    <url-pattern>/map</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>chart</servlet-name>
    <servlet-class>com.servlet.OChart</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>chart</servlet-name>
    <url-pattern>/chart</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>request</servlet-name>
    <servlet-class>com.servlet.ORequest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>request</servlet-name>
    <url-pattern>/request</url-pattern>
  </servlet-mapping>
</web-app>